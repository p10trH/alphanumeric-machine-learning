{
	"env": {
		"browser": true,
		"es6": true,
    "node": true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"rules": {
    "no-undef": "error",
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"windows"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		],
		"no-console": "warn"
	}
}